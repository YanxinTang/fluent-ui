{
  "name": "@tyx1703/fluent-ui",
  "version": "0.0.1",
  "description": "A Fluent UI toolkit implements by Vue 2.",
  "main": "src/index.js",
  "repository": "https://github.com/YanxinTang/fluent-ui",
  "homepage": "https://github.com/YanxinTang/fluent-ui#readme",
  "keywords": ["Fluent UI", "Vue"],
  "author": "tyx1703",
  "license": "MIT",
  "private": false,
  "scripts": {
    "lib:build": "webpack --config build/webpack.prod.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "test": "jest",
    "lint": "eslint --ext .js,.vue src test packages",
    "format": "prettier --write src/**/* test/**/* packages/**/* docs/**/*",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,vue}": [
      "eslint --fix"
    ],
    "**/*.{scss}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@vue/test-utils": "^1.0.3",
    "babel-jest": "^26.1.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "jest": "^26.1.0",
    "lint-staged": "^10.2.11",
    "markdown-it-container": "^3.0.0",
    "node-sass": "^4.14.1",
    "prettier": "2.0.5",
    "sass-loader": "^9.0.2",
    "standard-version": "^8.0.2",
    "vue-jest": "https://github.com/YanxinTang/vue-jest.git#master",
    "vue-loader": "^15.9.3",
    "vue-template-compiler": "^2.6.11",
    "vuepress": "^1.5.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.0.9"
  }
}
